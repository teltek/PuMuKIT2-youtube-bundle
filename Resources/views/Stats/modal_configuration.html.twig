<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
    <h3 class="modal-title text-center" id="myModalLabel">{% trans %}Youtube Configuration{% endtrans %}</h3>
</div>
<div class="modal-body">
    <table class="table">
        <tbody><tr>
            <th style="width: 10px">#</th>
            <th>Parameter</th>
            <th>Value</th>
        </tr>
            {% for parameter, value in youtubeConfiguration %}
                <tr>
                    <td>{{ loop.index }}.</td>
                    <td>{{ parameter }}</td>
                    <td>
                        {% for val in value %}
                            {{ val }}
                        {% else %}
                            {% if value is same as(false) %}
                                <i class="mdi-navigation-check"></i>
                            {% elseif value is same as(true) %}
                                <i class="mdi-navigation-close"></i>
                            {% else %}
                                {{ value }}
                            {% endif %}
                        {% endfor %}
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
